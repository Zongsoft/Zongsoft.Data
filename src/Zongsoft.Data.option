					<include name="container.*" />
					<include name="container.entry" />
					<exclude name="container.*" />
					<exclude name="container.entry" />
				</supports>

				<container name="" namespace="">
					<entities>
						<entity name="" />
					</entities>

					<commands>
						<command name="" />
					</commands>
				</container>
			</provider>
		</dataExecutor>
	</option>
</options>

<?xml version="1.0" encoding="utf-8" ?>

<options>
	<option path="/Data">
		<connectionStrings>
			<connectionString name="local:master" provider="mysql" value="">
				<property name="AccessMode" value="ReadWrite" />
			</connectionString>
			<connectionString name="local:slave.1" provider="mysql" value="">
